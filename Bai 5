import java.util.Scanner;
 
class Bai05 {
	static int min(int a, int b) {
		if (a < b) 
			return a;
		return b;
	}
 
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
 
        System.out.println(min(a, min(b, c)));
    }
}
