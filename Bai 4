import java.util.Scanner;
 
class Bai04 {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int y = sc.nextInt();
 
        if (y % 4 == 0 && y % 100 != 0)
        	System.out.println("Nam nhuan");
        else
        	System.out.println("Nam khong nhuan");
    }
}
